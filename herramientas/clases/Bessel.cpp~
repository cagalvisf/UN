#include <iostream>
#include <stdio.h>
#include <math.h>
#include <gsl/gsl_sf_bessel.h>



int main ()
{

  int x=0,n;
  double v=0.0;
  std::cout.precision(8);
  std::cout.setf(std::ios::scientific);

  for (int ii=0;ii<=500,ii++)
    {
      x=0.01*ii;
      double y=gsl_sf_bessel_Jn0 (x);
      std::cout<<x<<"\t"<<y<<std::endl;
    }

  
  return 0;
  
}
