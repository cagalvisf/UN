#include<iostream>
#include<cmath>
double euler(double x, int N);
int factorial(int N);
int main()
{
  
  return 0;
}

double euler(double x, int N)
{
  double suma=0.0;
  for(int ii=0;ii<=N;ii++)
    {
      suma+= std::pow(-1,ii)*std::pow(x,ii)/factorial(ii);
    }
}
int factorial(int N)
{
  return N*(N-1);
}
