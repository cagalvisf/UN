# include <iostream>
# include <array>
# include <cmath>

int main(void)
{
  const int n=10;
  std::array<double, n> x {1.22, 123.156, 1546.165, 13.16, 12.22, 22.4541,
      0.51484, 12.54, 1.15,132 };
  
  double l,t, prom,de;
  l=0;
  t=0;
  de=0;
  prom=0;
  for (int ii=0;ii<n;ii++)
    {
      l += x[ii];
    }
  
  prom=l/(n);
  
  for(int jj=0 ; jj < n ; jj++)
    {
      t += (x[jj] - prom)*(x[jj] - prom);
      
    }
  de=std::sqrt(t/(n-1));
  std::cout<< l <<std::endl;
  std::cout<< de <<std::endl;
  std::cout<< prom  <<std::endl;

  
  return 0;
}
